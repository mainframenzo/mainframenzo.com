  </div>
  <% 
    slides.forEach(function(slide) {
  %>
  <figure>
    <div class='image-container'>
      <image loading='lazy' class='one_photo' src='<%= slide.filePath %>'/> <!-- FIXME  ondblclick='this.requestFullscreen()' shows black -->
      <figcaption><%- slide.description %></figcaption>
    </div>
  </figure>
  <%
    });
  %>
</div> 