<div class='posts'>
  <div class='mascot'>
    <image loading='lazy' class='palm_tree' src='images/palm_tree.png' />
  </div>
  <div class='post_count'> 
    <h2><%= posts.length %></h2>
  </div>
  <div class='post_list'>
    <% 
    if (posts) {
      posts.forEach(function(post) {
    %>
    <div class='post_item'>
      <div class='row'>
        <div class='column post_info'> 
          <a href='/posts/<%= post.postInfo.title %>.html'><%= post.postInfo.title %></a><span><%= post.postInfo.publishedOn %></span>
        </div>
      </div>
    </div>
    <%
      });
    }
    %>
  </div>
</div>